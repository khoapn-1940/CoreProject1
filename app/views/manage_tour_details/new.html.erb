<%= form_for(@tour_details, url: new_tour_details_path) do |f| %>
  <%= render "shared/form_error_messages", object: @tour_details %>
  <%= f.label "Price" %>
  <%= f.text_field :price %>
  <%= f.label "Tour Total" %>
  <%= f.text_field :tour_total %>
  <%= f.label "User ID" %>
  <%= f.text_field :user_id %>
  <br>
  <br>
  <%= f.label "Time Start" %>
  <span class="datetime"><%= f.datetime_select :time_start , ampm: true , :class => "form-control"%><span/>
    <br>
    <%= f.label "Time End" %>
    <span class="datetime"><%= f.datetime_select :time_end , ampm: true , :class => "form-control"%><span/>
      <br>
      <%= f.label "Tour ID" %>
      <%= f.text_field :tour_id %>
      <%= f.submit "Create", class: "btn btn-primary" %>
    <% end %>
