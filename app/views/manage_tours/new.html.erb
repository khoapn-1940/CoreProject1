<%= form_for(@tour,url: new_tour_path) do |f| %>
    <%= render "shared/form_error_messages", object: @tour %>
    <%= f.label "Name" %>
    <%= f.text_field :name %>
    <%= f.label "Description" %>
    <%= f.text_field :description %>
    <%= f.label "User ID" %>
    <%= f.text_field :user_id %>
    <%= f.label "Category ID" %>
    <%= f.text_field :category_id %>
    <%= f.submit "Create Tour", class: "btn btn-primary" %>
<% end %>
  